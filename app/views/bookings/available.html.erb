
<div>
    <% @booked.each do |booking| %>
        <tr>
            <td><%= booking.user.name %></td>
            <td><%= booking.user.department %></td>
            <td><%= booking.room.location %></td>
            <td><%= booking.room.capacity %></td>
            <td><%= booking.room.classification %></td>
            <td><%= booking.startdate %></td>
            <td><%= booking.enddate %></td>
        </tr>
    <% end %>
    
</div>



<%= pie_chart([
  {name: "Series A", data: 25},
  {name: "Series B", data: 100},
  {name: "Series C", data: 200},
  {name: "Series D", data: 125}
], legend: "left") %>